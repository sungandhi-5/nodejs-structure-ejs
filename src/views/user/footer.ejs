<div class="toast-container position-fixed bottom-0 start-0 end-0 m-auto p-3"></div>

</body>


<script src="<%= process.env.NODE_URL + 'assets/js/jquery.min.js'%>"></script>
<script src="<%= process.env.NODE_URL + 'assets/js/jquery-ui.min.js'%>"></script>
<script src="<%= process.env.NODE_URL + 'assets/js/jquery.form.min.js' %>" type="text/javascript"></script>
<script src="<%= process.env.NODE_URL + 'assets/js/bootstrap.min.js'%>"></script>
<script src="<%= process.env.NODE_URL + 'assets/js/common.min.js'%>"></script>

<script>
    $("#<%= body['id'] %>").addClass("active");
    $("#<%= body['id'] %>").parent().parent().addClass('show');
    let parentId = $("#<%= body['id'] %>").parent().parent().attr('id');
    $("a[data-bs-target='#" + parentId +"']").removeClass("collapsed");
</script>
<% if (typeof footer['js'] !== 'undefined') { %>
    <% for (let i = 0; i < footer.js.length; i++) { %>
       <% if (footer['js'][i].startsWith("https://") || footer['js'][i].startsWith("http://")) { %>
            <script type="text/javascript" src="<%=footer['js'][i]%>"></script>
        <% } else { %>
            <script type="text/javascript" src="<%= process.env.NODE_URL + 'assets/js/' + footer['js'][i] %>"></script>
        <% } %>
    <% } %>
 <% } %>



</html>